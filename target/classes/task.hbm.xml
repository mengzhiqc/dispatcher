<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0/EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.aifang.model.Task"  table="task">
		<id name="id"  column="id">
			<generator class="native" />
		</id>
		<property name="username"  ><column name="username"/>
		</property>
		<property name="taskname"  ><column name="taskname" />
			</property>
		<property name="score" >
			<column name="score"/>
		</property>
		
		<property name="flag" >
		<column name="flag"/>
		</property>	
		 <property name="create_time" >
		<column name="create_time" />
		</property>	
		<property name="last_update_time" >	
		<column name="last_update_time" />
		</property>
		
	</class>
</hibernate-mapping>